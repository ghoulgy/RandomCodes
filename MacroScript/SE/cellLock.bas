Sub Locking(ByVal WS1 As Object, ByVal WS2 As Object, ByVal WS3 As Object)
    WS1.Unprotect
    WS1.Cells.Locked = False
    WS1.Cells.Locked = True
    WS1.Protect UserInterfaceOnly:=False
    WS2.Unprotect
    WS2.Cells.Locked = False
    WS2.Cells.Locked = True
    WS2.Protect UserInterfaceOnly:=False
    WS3.Unprotect
    WS3.Cells.Locked = False
    WS3.Cells.Locked = True
    WS3.Protect UserInterfaceOnly:=False
End Sub

